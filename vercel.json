{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"installCommand": "cd internal/site && make vercel-install",
	"buildCommand": "cd internal/site && make vercel-build",
	"outputDirectory": "internal/site/app/__dist/static/assets/public",
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/api"
		}
	],
	"headers": [
		{
			"source": "/river_out_(.*).(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	]
}
