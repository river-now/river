{
  "name": "hwy",
  "version": "0.12.0",
  "author": {
    "name": "Samuel J. Cook"
  },
  "description": "A highly flexible fullstack web framework",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup-node index.ts --format esm --dts",
    "dev": "rm -rf dist && tsup-node index.ts --format esm --dts --watch",
    "publish-pre": "npm publish --tag pre",
    "publish-non-pre": "npm publish",
    "update-latest": "pnpm update --latest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "devalue": "^4.3.2"
  },
  "devDependencies": {
    "@hwy-js/build": "workspace:*",
    "@hwy-js/dev": "workspace:*",
    "@types/node": "^20.12.7",
    "tsup": "^8.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sjc5/hwy",
    "directory": "packages/core"
  },
  "homepage": "https://hwy.dev",
  "bugs": "https://github.com/sjc5/hwy/issues",
  "keywords": [
    "simple",
    "lightweight",
    "flexible",
    "framework",
    "metaframework",
    "fullstack",
    "web",
    "typescript",
    "unjs",
    "h3",
    "router",
    "nested",
    "routing",
    "jsx",
    "tsx",
    "nextjs",
    "remix",
    "react"
  ],
  "sideEffects": false
}
