{
  "name": "@hwy-js/react",
  "version": "0.8.0",
  "author": {
    "name": "Samuel J. Cook"
  },
  "description": "React utilities for Hwy framework",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup-node index.ts --format esm --dts",
    "dev": "rm -rf dist && tsup-node index.ts --format esm --dts --watch",
    "publish-pre": "npm publish --tag pre",
    "publish-non-pre": "npm publish",
    "update-latest": "pnpm update --latest",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sjc5/hwy",
    "directory": "packages/react"
  },
  "homepage": "https://hwy.dev",
  "bugs": "https://github.com/sjc5/hwy/issues",
  "sideEffects": false
}
